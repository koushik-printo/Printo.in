$(function () {
    var A = "<img src=\'images/cal_img/cal_pips/pipsbusinessblack.png\' style=\'width:1px; height:1px; display:none;\'><img src=\'images/cal_img/cal_pips/pipsbusinesswhite.png\'  style=\'width:1px; height:1px; display:none;\' ><img src=\'images/cal_img/cal_pips/pipsfunblack.png\' style=\'width:1px; height:1px; display:none;\'><img src=\'images/cal_img/cal_pips/pipsclassic.png\' style=\'width:1px; height:1px; display:none;\'><img src=\'images/cross.png\'  style=\'width:1px; height:1px; display:none;\'><img src=\'images/blueBack.jpg\'  style=\'width:1px; height:1px; display:none;\'>";
    $('#hiddenImg').html(A);
    $('#name').change(function () {
        $("#name").css('color', 'black');
        var B = $("#name").val();
        $("#pips_name").val(B)
    });
    $('#fullname').change(function () {
        $("#fullname").css('color', 'black');
        var B = $("#fullname").val();
        $("#pips_fullname").val(B)
    });
    $('#name').click(function () {
        $("#name").css('color', 'black');
        var B = $("#name").val();
        if (B == 'Please Fill All Fields Again' || B == 'Please Enter the Name' || B == 'Please Enter Valid Name') $("#name").val('')
    });
    $('#fullname').click(function () {
        $("#fullname").css('color', 'black');
        var B = $("#fullname").val();
        if (B == 'Please Fill All Fields Again' || B == 'Please Enter the FullName' || B == 'Please Enter Valid FullName') $("#fullname").val('')
    });
    $('#name').keydown(function () {
        $("#name").css('color', 'black');
        var B = $("#name").val();
        if (B == 'Please Fill All Fields Again' || B == 'Please Enter the Name' || B == 'Please Enter Valid Name') $("#name").val('')
    });
    $('#fullname').keydown(function () {
        $("#fullname").css('color', 'black');
        var B = $("#fullname").val();
        if (B == 'Please Fill All Fields Again' || B == 'Please Enter the FullName' || B == 'Please Enter Valid FullName') $("#fullname").val('')
    });
    $('#pips_cal_type').change(function () {
        var B = $("#pips_cal_type option:selected").val();
        switch (B) {
        case '17':
            $("#imgType").attr('src', 'images/cal_img/cal_pips/pipsbusinessblack.png');
            break;
        case '18':
            $("#imgType").attr('src', 'images/cal_img/cal_pips/pipsbusinesswhite.png');
            break;
        case '19':
            $("#imgType").attr('src', 'images/cal_img/cal_pips/pipsfunblack.png');
            break;
        case '20':
            $("#imgType").attr('src', 'images/cal_img/cal_pips/pipsclassic.png');
            break
        }
    })
});